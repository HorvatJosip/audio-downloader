{
	"Initializer": {
		"Services": {
			"ConfigurationService": {
				"FilePath": "Configuration.dat",
				"InitialConfiguration": {
					"InfoBoxDuration": 10,
					"Language": "en-US",
					"VideoDownloadDirectory": "C:\\Videos",
					"AudioDownloadDirectory": "C:\\Songs",
					"DownloadRetryCount": 50,
					"DeleteVideoAfterMP3Conversion": true,
					"DeleteAudioSplitSourceFile": true,
					"Volume": 0.5
				}
			},
			"Localizer": {
				"Cultures": [ "hr-HR", "en-US" ]
			},
			"PathManager": {
				"AppName": "Audio Downloader"
			},
			"ResourceManager": {
				"Location": "Resources"
			},
			"ToolTipGetter": {
				"KeyMap": {
					"Maximize": "TitleBar_Maximize_ToolTip",
					"Restore": "TitleBar_Restore_ToolTip"
				}
			}
		}
	}
}
